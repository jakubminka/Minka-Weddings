<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirect jakubminka.cz and www.jakubminka.cz to www.minka-weddings.cz
  RewriteCond %{HTTP_HOST} ^(www\.)?jakubminka\.cz$ [NC]
  RewriteRule ^(.*)$ https://www.minka-weddings.cz/$1 [R=301,L]

  # SPA routing - pokud soubor/slo≈æka neexistuje, serve index.html
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.html [QSA,L]
</IfModule>
