<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

# Redirect jakubminka.cz to minka-weddings.cz
RewriteCond %{HTTP_HOST} ^(www\.)?jakubminka\.cz$ [NC]
RewriteRule ^(.*)$ https://minka-weddings.cz/$1 [R=301,L]

# React SPA routing
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ index.html [QSA,L]
</IfModule>
